    "            m=self.m\n",
    "            x1 = self.conv9(x1)\n",
    "        else:\n",
    "            x1 = self.conv2(x1)\n",
    "        x1 = self.bn(x1)\n",
    "        x1 = tf.nn.relu(x1)\n",
    "        x1 = self.sigmoid(x1)\n",
    "        x1= tf.keras.layers.UpSampling2D(size=(x_origin.shape[1],x_origin.shape[2]),interpolation = 'nearest')(x1)\n",
