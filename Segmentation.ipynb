    "        spatial22 = tf.concat([avgpool22,maxpool22],axis=3)\n",
    "        spatial22 = self.spatial(spatial22)\n",
    "        spatial22 =self.sigmoid(spatial22)\n",
    "        x_22  =tf.multiply(x_22,spatial22)\n",
